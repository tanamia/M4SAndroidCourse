<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.aminatambengue.disney_princess.MainActivityFragment"
    tools:showIn="@layout/activity_main"
    android:background="?android:attr/textColorHighlightInverse">

    <GridLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/disney_princess">

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/disney3"
            android:id="@+id/imageView6"
            />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/disney1"
            android:id="@+id/imageView4"
            android:layout_row="1"
            android:layout_gravity="right|center_vertical"
            />

        <ImageView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/disney2"
            android:id="@+id/imageView5"
            android:layout_row="1"
            android:layout_gravity="right"
            />


    </GridLayout>


</android.support.constraint.ConstraintLayout>
